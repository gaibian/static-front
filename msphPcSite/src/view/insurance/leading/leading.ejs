        <li class="classify_list">
            <div class="img_box">
                <img width="100%" height="100%" src="<%= require('../../../public-resource/imgs/insurance_classify_img5.png') %>">
            </div>
            <p class="tit">品牌实力</p>
            <p class="note_span">
