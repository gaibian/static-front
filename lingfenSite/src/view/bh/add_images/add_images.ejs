<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>添加影像</title>
    <%= layout() %>
</head>
<body>
<main class="page">
    <a class="return_page_btn"></a>
    <section class="page_header_container">添加影像</section>
    <section class="remarks_text">
        <i class="remark_bg"></i>
        核保退回修改,胡婷婷
    </section>
    <section class="form" data-btn="upload_btn">
        <div class="input_box">
            <input type="text" placeholder="备注信息" data-toggle="true" checkType="empty" data-error="请输入备注信息">
        </div>
    </section>
    <section class="add_upload_photo_container">
        <div class="file_box">
            <img width="100%" height="100%" src="<%= require('../../../public-resource/imgs/add_images_img.jpg') %>">
            <input type="file">
        </div>
        <div class="file_box">
            <img width="100%" height="100%" src="<%= require('../../../public-resource/imgs/add_images_img1.jpg') %>">
            <input type="file">
        </div>
    </section>
    <button class="upload_btn">确认上传</button>
</main>
</body>
<script>
    $(function(){
      verify([{
        el:'.form',
        success:function(){
          console.log('tongg');
        }
      }]);
      //图片的上传
      var addFileBox = $('.add_upload_photo_container');
      var pic = function(){
        picUpLoad({
          el:'.file_box',
          url:'http://bh.upingou.com/index.php?g=Wap&m=Pay&a=upload',
          callback(res,self,index){
            console.log(res);
            var img = self.querySelector('img');
            img.src = res[0].msg;
            var fileBox = $('.file_box');
            if(index === fileBox.length - 1){
              //是最后一个
              var addDom = '<div class="file_box">\n' +
                  '            <img width="100%" height="100%" src="data:image/jpeg;base64,">\n' +
                  '            <input type="file">\n' +
                  '        </div>';
              addFileBox.append(addDom);
              pic();
            }
          },
        })
      }
      pic();
    })
</script>
</html>