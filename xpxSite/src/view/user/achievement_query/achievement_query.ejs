<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <%= layout %>
    <title>业绩查询</title>
</head>
<body>
<section class="page_container">
    <form class="form" data-btn="submit_btn">
        <div class="form_input">
            <label class="input_title">查询代码</label>
            <div class="input_box">
                <input type="text" placeholder="填入投保人手机号" data-toggle="true" checkType="empty" data-error="请输入正确的查询代码">
            </div>
        </div>
        <div class="form_input verify_input">
            <label class="input_title">图形验证码</label>
            <div class="input_box">
                <input type="text" placeholder="填入图片验证码" data-toggle="true" checkType="empty" data-error="请输入图形验证码">
                <div class="verify_img">
                    <img width="100%" height="100%" src="<%= require('../../../public-resource/imgs/verify_img.jpg')%>">
                </div>
            </div>
        </div>
    </form>
    <section class="btm_btn_box">
        <button class="submit_btn">提交</button>
    </section>
</section>
</body>
</html>